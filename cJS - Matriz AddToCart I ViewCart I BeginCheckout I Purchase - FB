function() {           
  var products = {{DLV - ecommerce.items}}; //matriz de produtos addtocart no padr√£o FB  
  var tam = products.length
  var indice=0
  var viarray =[{"id":products[indice].item_variant,
                 "title":products[indice].item_name,
                 "brand": products[indice].item_brand,                              
                 "quantity":parseFloat(products[indice].quantity),
                 "item_price":products[indice].price
                }];
  var tam=tam-1
    while (tam !=0){
       var indice=indice+1
       viarray.push ({"id":products[indice].item_variant,
                      "title":products[indice].item_name,
                      "brand": products[indice].item_brand,                                 
                      "quantity":parseFloat(products[indice].quantity),
                      "item_price":products[indice].price
                      });
       var tam=tam-1
                    }  
  return viarray;
}
